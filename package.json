{
    "name": "oleg-brand-kit",
    "version": "2.2.0",
    "description": "A place to store global brand information for use across various digital properties (Sass colour variables, typography etc)",
    "main": "index.js",
    "scripts": {
        "stylelint": "stylelint --rd --rdd \"**/*.{css,scss,sass}\" -s scss",
        "stylelint-fix": "stylelint --rd --rdd \"**/*.{css,scss,sass}\" -s scss --fix",
        "build": "node-sass --output-style compressed test.scss test.css"
    },
    "repository": "git@github.com:AgePartnership/oleg-brand-kit.git",
    "author": "Sam Hardacre <me@nocturnalmonkey.com>",
    "license": "MIT",
    "dependencies": {
        "oleg-starter-pack": "^1.0.3"
    },
    "devDependencies": {
        "ap-stylelint-config": "github:agepartnership/ap-stylelint-config",
        "node-sass": "^6.0.0"
    }
}
